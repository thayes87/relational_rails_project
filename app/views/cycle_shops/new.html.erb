<%= form_with url:'/cycle_shops', method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :rental_program %>
  <%= form.text_field :rental_program %>

  <%= form.label :bike_capacity %>
  <%= form.text_field :bike_capacity %>
 
  <%= form.submit "Create Cycle Shop" %>
<% end %>